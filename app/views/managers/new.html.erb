<%= simple_form_for(@manager, :html => {:class => 'form-inline'}) do |f| %>
  <%= f.input :title, placeholder: 'Title', error: false %>
  <%= f.input :name, placeholder: 'Name', error: false %>
  <%= f.input :address, placeholder: 'Address', error: false %>
  <%= f.input :phone_number, placeholder: 'Phone Number', error: false %>
  <%= f.button :submit, 'Create' %>
<% end %>
